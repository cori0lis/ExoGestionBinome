{% extends 'base.html.twig' %}

{% block contenu %}
<a type="button" href="{{ path('stagiaire')}}" class="btn" style="padding-left: 0;"><i class="fas fa-arrow-left" style="font-size: 3rem"></i></a>
	<p>{{ stagiaire.nom ~ ' ' ~ stagiaire.prenom }}</p>
	<p>{{ stagiaire.civilite }}</p>
	<p>{{ stagiaire.email }}</p>
	<p>{{ stagiaire.telephone }}</p>
	<p>{{ stagiaire.dateNaissance | date ('d/m/Y') }}</p>
	<p>{{ stagiaire.ville }}</p>
	<p>{{ stagiaire.adresse }}</p>
	<p>{{ stagiaire.cp }}</p>
    {# <a type="button" class="btn btn-warning" href="{{ path('app_changepassword', {'id':app.user.id}) }}">Modifier MDP</a> #}
{% endblock %}
