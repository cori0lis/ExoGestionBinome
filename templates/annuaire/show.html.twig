{% extends 'base.html.twig' %}

{% block contenu %}
	<p>{{ utilisateur.nom ~ ' ' ~ utilisateur.prenom }}</p>
	<p>{{ utilisateur.civilite }}</p>
	<p>{{ utilisateur.email }}</p>
	<p>{{ utilisateur.telephone }}</p>
	<p>{{ utilisateur.dateNaissance | date ('d/m/Y') }}</p>
	<p>{{ utilisateur.ville }}</p>
	<p>{{ utilisateur.adresse }}</p>
	<p>{{ utilisateur.cp }}</p>
    <a type="button" class="btn btn-warning" href="">Modifier MDP</a>
{% endblock %}
