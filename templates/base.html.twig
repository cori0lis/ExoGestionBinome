<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link href="https://stackpath.bootstrapcdn.com/bootswatch/4.5.2/lux/bootstrap.min.css" rel="stylesheet">
			<link href="{{ asset('css/style.css') }}" type="text/css" rel="stylesheet">
		{% endblock %}
	</head>
	<body>
		{% block body %}
			{# {% if error %}
			        <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
			    {% endif %}
			
			    
			
			    {% for label, messages in app.flashes %}
			        {% for message in messages %}
			            {% if message == ('warning') %}
			                <div class="flash-{{ label }} alert alert-success">
			                    {{ message }}
			                </div>
			            {% endif %}
			        {% endfor %}
			    {% endfor %} #}
			{% if app.user %}
				{# {% if is_granted('ROLE_ADMIN') %} #}
				<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
					<a class="navbar-brand" href="{{ path('utilisateur') }}">
						<img src="{{ asset('images/sharpener.svg') }}" width="30" height="30" alt="">
						ExoGestionBinome</a>
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>

					<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<form class="form-inline my-2 my-lg-0">
							<input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
							<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
						</form>


						<ul class="navbar-nav">
							<li class="nav-item">
								<p class="nav-link">Bienvenue
									{{ app.user.prenom ~ ' ' ~ app.user.nom }}</p>
							</li>
							<li class="nav-item active">
								<a id="monprofilcolor" class="nav-link" href="{{ path('utilisateur_show', {'id':app.user.id}) }}">MON PROFIL
								</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ path('app_logout') }}">DECONNEXION</a>
							</li>
						</ul>


					</div>
				</nav>
				<div class="wrapper0">
					<div class="wrapper1">
						<div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
							<a id="coloredselect1" class="nav-link btn" href="{{ path('stagiaire')}}">Stagiaires</a>
							<a id="coloredselect2" class="nav-link btn" href="{{ path('formation')}}">Formations</a>
							<a id="coloredselect3" class="nav-link btn" href="{{ path('utilisateur')}}">Employ√©s</a>
							{% if is_granted('ROLE_ADMIN') %}
								<a id="coloredselect4" class="nav-link btn" href="{{ path('categorie')}}">Categories</a>
								<a id="coloredselect5" class="nav-link btn" href="{{ path('session')}}">Sessions</a>
								<a id="coloredselect6" class="nav-link btn" href="{{ path('module')}}">Modules</a>
							{% endif %}
						</div>




						<div id="map"></div>
  



				{% block javascripts %}

 <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <script defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA8t0-agafpUqfs8-Hfx7Gw8gMGP6sq8SA&callback=initMap">
</script>
    <style type="text/css">
      /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
      #map {
		height: 30em;
		width: 100%;
      }
    </style>
    <script>
      let map;

      function initMap() {
        map = new google.maps.Map(document.getElementById("map"), {
          center: { lat: 48.578980, lng: 7.760070 },
          zoom: 8,
        });
      }
	</script>
	


	
					</div>
					<div class="wrapper2">
						{% block contenu %}{% endblock %}
					</div>
				</div>


   
 
    




					<script src = "https://code.jquery.com/jquery-3.5.1.min.js"
integrity = "sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
crossorigin = "anonymous" >
					</script>
				{% endblock %}
				<script src="https://kit.fontawesome.com/96b70f9c88.js" crossorigin="anonymous"></script>
			{% else %}

				<h1>Veuillez vous
					<a type="button" class="btn btn-success" href="{{ path('app_login') }}">CONNECTER</a>
				</h1>

				{# {% endif %} #}
			{% endif %}
		{% endblock %}
	</body>
</html>
